{
  "schemaVersion": 1,
  "id": "nexia",
  "version": "${version}",

  "name": "Nexia",
  "description": "The mod for the Nexia CTS server.",
  "authors": [
    "NotCoded"
  ],
  "contributors": [
    "Shinkume",
    "betterclient"
  ],
  "contact": {
    "homepage": "https://nexia.mcserver.us/",
    "sources": "https://github.com/nexia-cts/Nexia-Mod"
  },

  "license": "LGPL-3.0",
  "icon": "assets/nexia/logo.png",

  "environment": "server",
  "entrypoints": {
